{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "id": "http://jsonschema.net",
  "required": false,
  "properties": {
    "version": {
      "type": "string",
      "required": true,
      "pattern": "^.*(\\d|-[A-Z0-9]+)$"
    },
    "buildTime": {
      "type": "number",
      "required": true
    }
  }
}