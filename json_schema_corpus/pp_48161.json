{
  "id": "http://schema.myrtana.sk/metadata/dublincore.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Dublin Core metadata basic",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "contributor": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/entities/entityOrPerson.json"
          }
        ]
      }
    },
    "coverage": {
      "type": "array",
      "minItems": 0
    },
    "creator": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/entities/entityOrPerson.json"
          }
        ]
      },
      "minItems": 0
    },
    "date": {
      "type": "object",
      "properties": {
        "updated_at": {
          "type": "http://schema.myrtana.sk/support/datetime.json"
        },
        "created_at": {
          "type": "http://schema.myrtana.sk/support/datetime.json"
        },
        "published_at": {
          "type": "http://schema.myrtana.sk/support/datetime.json"
        }
      }
    },
    "description": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/text/coupleOfParagraphs.json"
          }
        ]
      },
      "minItems": 0
    },
    "format": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/text/shortString.json"
          }
        ]
      }
    },
    "identifier": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/text/shortString.json"
          }
        ]
      }
    },
    "language": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/support/languageRFC4646.json"
          }
        ]
      }
    },
    "publisher": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/entities/entityOrPerson.json"
          }
        ]
      },
      "minItems": 0
    },
    "relation": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/support/url.json"
          },
          {
            "$ref": "http://schema.myrtana.sk/support/bibliographicBibTeXLike.json"
          }
        ]
      },
      "minItems": 0
    },
    "rights": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/support/url.json"
          },
          {
            "$ref": "http://schema.myrtana.sk/entities/entityOrPerson.json"
          }
        ]
      },
      "minItems": 0
    },
    "source": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/support/url.json"
          },
          {
            "$ref": "http://schema.myrtana.sk/entities/entityOrPerson.json"
          }
        ]
      },
      "minItems": 0
    },
    "subject": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/text/tag.json"
          }
        ]
      },
      "minItems": 0
    },
    "title": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/text/shortString.json"
          }
        ]
      }
    },
    "type": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "$ref": "http://schema.myrtana.sk/text/shortString.json"
          }
        ]
      }
    }
  }
}