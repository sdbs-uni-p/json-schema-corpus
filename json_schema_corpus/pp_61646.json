{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "PayloadUUID": {
      "default": "",
      "pattern": "^[0-9A-Za-z]{8}-[0-9A-Za-z]{4}-[0-9A-Za-z]{4}-[0-9A-Za-z]{4}-[0-9A-Za-z]{12}$",
      "type": "string",
      "description": "Unique identifier for the payload (format 01234567-89AB-CDEF-0123-456789ABCDEF)"
    },
    "PayloadDescription": {
      "default": "Per-App VPN Payload",
      "type": "string",
      "description": "Description of the payload"
    },
    "PayloadOrganization": {
      "type": "string",
      "description": "This value describes the issuing organization of the profile, as displayed to the user"
    },
    "PayloadIdentifier": {
      "default": "com.apple.vpn.managed.appmapping",
      "type": "string",
      "description": "A unique identifier for the payload, dot-delimited.  Usually root PayloadIdentifier+subidentifier"
    },
    "PayloadDisplayName": {
      "default": "Per-App VPN Payload",
      "type": "string",
      "description": "Name of the payload"
    },
    "PayloadType": {
      "default": "com.apple.vpn.managed.appmapping",
      "type": "string",
      "description": "The type of the payload, a reverse dns string"
    },
    "AppLayerVPNMapping": {
      "items": {
        "type": "dict",
        "description": "Bundle ID to VPN UUID mapping",
        "properties": {
          "Identifier": {
            "type": "string",
            "description": "The app bundle ID."
          },
          "VPNUUID": {
            "type": "string",
            "description": "The VPNUUID of the Per-App VPN defined in a Per-App VPN payload."
          }
        }
      },
      "type": "array",
      "description": "An array of mapping dictionaries."
    },
    "PayloadVersion": {
      "default": 1,
      "type": "number",
      "description": "The version of the whole configuration profile."
    }
  },
  "title": "com.apple.vpn.managed.appmapping"
}