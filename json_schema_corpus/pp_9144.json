{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Glob Publisher",
  "id": "glob_publisher",
  "additionalProperties": false,
  "properties": {
    "publisher_type": {
      "type": "string",
      "enum": [
        "fromglob-pub"
      ],
      "default": "fromglob-pub"
    },
    "globexpression": {
      "type": "string"
    },
    "outputkey": {
      "type": "string"
    }
  },
  "required": [
    "publisher_type",
    "globexpression",
    "outputkey"
  ]
}