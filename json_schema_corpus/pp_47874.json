{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "id": "https://inowas.com/schema/",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
    },
    "start": {
      "type": "string",
      "minLength": 3,
      "maxLength": 255
    },
    "end": {
      "type": "string"
    },
    "initial_steady": {
      "type": "boolean"
    }
  },
  "required": [
    "id",
    "start",
    "end"
  ]
}