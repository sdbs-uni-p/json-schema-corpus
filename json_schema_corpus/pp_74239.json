{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "allOf": [
    {
      "$ref": "../definitions/user.json#/definitions/user-input"
    }
  ],
  "required": [
    "_id",
    "firstname",
    "lastname"
  ],
  "example": {
    "_id": "jane.doe@gmail.com",
    "firstname": "Jane",
    "lastname": "Doe",
    "email": "jane.doe@gmail.com",
    "root": false
  }
}