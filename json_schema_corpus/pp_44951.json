{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "version": "1.0.1",
  "type": "object",
  "properties": {
    "object": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "source": {
            "type": "string"
          },
          "page": {
            "type": "integer"
          },
          "size": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "siege weapon",
              "generic"
            ]
          },
          "ac": {
            "type": "string"
          },
          "hp": {
            "type": "string"
          },
          "immune": {
            "type": "string"
          },
          "entries": {
            "type": "array",
            "items": {
              "$ref": "/Entry"
            }
          },
          "actionEntries": {
            "type": "array",
            "items": {
              "$ref": "/Entry"
            }
          },
          "tokenURL": {
            "type": "string"
          }
        },
        "additionalProperties": false,
        "required": [
          "name",
          "source",
          "size",
          "type",
          "ac",
          "hp",
          "immune",
          "entries"
        ]
      }
    }
  },
  "additionalProperties": false
}