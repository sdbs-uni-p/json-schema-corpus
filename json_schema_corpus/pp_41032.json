{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "SubnetId",
    "AllocationId"
  ],
  "properties": {
    "SubnetId": {
      "type": "string",
      "pattern": "^[a-z][a-z0-9\\-/]+$"
    },
    "AllocationId": {
      "type": "string",
      "pattern": "^[a-z][a-z0-9\\-/]+$"
    }
  }
}