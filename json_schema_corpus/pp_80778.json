{
  "allow_all": true,
  "experiment": null,
  "fullname": "Primary Dataset",
  "is_deposit": false,
  "jsonschema": {
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Primary Dataset",
    "type": "object",
    "description": "Information concerning the CMS Primary Dataset used",
    "properties": {
      "dataset_metadata": {
        "type": "object",
        "properties": {
          "other_ids": {
            "type": "number",
            "description": "Provide dataset ID e.g. 1853590",
            "title": "Dataset ID"
          },
          "run_number": {
            "type": "number",
            "description": "e.g. 146242",
            "title": "Run #"
          },
          "description": {
            "title": "Description",
            "type": "string",
            "description": "Provide a description for the primary dataset",
            "format": "textarea"
          },
          "date_modified": {
            "hidden": "true",
            "title": "Modified",
            "type": "string",
            "format": "datetime"
          },
          "persistent_identifiers": {
            "items": {
              "$ref": "https://analysispreservation.cern.ch/schemas/definitions/identifiers-v0.0.1.json"
            },
            "type": "array",
            "description": "",
            "title": "Persistent Identifiers"
          },
          "date_available": {
            "type": "number",
            "description": "Provide the year the primary dataset will be available in public. e.g. 2014",
            "title": "Year Availble"
          },
          "date_issued": {
            "title": "Issued",
            "type": "string",
            "description": "Provide date/time the primary dataset was issued",
            "format": "datetime"
          },
          "numbers": {
            "title": "Numbers",
            "type": "object",
            "properties": {
              "files": {
                "type": "number",
                "title": "Number of files"
              },
              "blocks": {
                "type": "number",
                "title": "Number of blocks"
              },
              "events": {
                "type": "number",
                "title": "Numbers of events"
              },
              "extend": {
                "type": "number",
                "title": "Extend"
              },
              "lumis": {
                "type": "number",
                "title": "Number of lumis"
              }
            },
            "description": "Numbers concerning various stuff"
          },
          "licence": {
            "type": "string",
            "description": "e.g. CC0 waiver",
            "title": "Licence"
          },
          "type": {
            "type": "string",
            "description": "Provide type of item e.g. 'data'",
            "title": "Type"
          },
          "dataset_title": {
            "autocomplete": true,
            "type": "string",
            "propertyOrder": 201,
            "description": "e.g. /Mu/Run2010B-Apr21ReReco-v1/AOD",
            "title": "Title"
          }
        }
      },
      "event_selection": {
        "type": "object",
        "properties": {
          "event_filter": {
            "enum": [],
            "type": "string",
            "title": "Event Filter"
          },
          "custom_event_filter": {
            "dependencies": [
              "event_filter"
            ],
            "type": "string",
            "title": " "
          },
          "reference": {
            "type": "string",
            "title": "Reference"
          }
        },
        "title": "Event Filter"
      },
      "json_file": {
        "type": "string",
        "title": "json File for Validated Runs"
      },
      "selection_triggers": {
        "items": {
          "title": "Trigger",
          "type": "object",
          "properties": {
            "run_period": {
              "enum": [],
              "type": "string",
              "title": "Run Period"
            },
            "trigger": {
              "enum": [],
              "type": "string",
              "dependencies": [
                "run_period",
                "element"
              ],
              "title": "Trigger"
            },
            "trigger_efficiency": {
              "required": "true",
              "type": "string",
              "title": "Trigger Efficiency"
            },
            "element": {
              "enum": [],
              "type": "string",
              "dependencies": [
                "run_period"
              ],
              "title": "Element"
            }
          },
          "id": "trigger"
        },
        "type": "array",
        "description": "Add triggers here",
        "title": "Selection Triggers"
      }
    }
  }
}