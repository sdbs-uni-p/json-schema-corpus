{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "#group_base",
  "type": "object",
  "required": [
    "objectType"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "objectType": {
      "enum": [
        "Group"
      ]
    },
    "member": {
      "type": "array",
      "items": {
        "$ref": "#agent"
      }
    }
  }
}