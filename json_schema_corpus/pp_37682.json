{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://project-open-data.github.io/schema#",
  "description": "Common Core Metadata Schema",
  "type": "object",
  "required": [
    "title",
    "description",
    "keyword",
    "modified",
    "publisher",
    "contactPoint",
    "mbox",
    "identifier",
    "accessLevel"
  ],
  "properties": {
    "accessLevel": {
      "enum": [
        "public",
        "restricted public",
        "non-public"
      ]
    },
    "accessLevelComment": {
      "type": "string"
    },
    "accrualPeriodicity": {
      "enum": [
        "Annual",
        "Bimonthly",
        "Semiweekly",
        "Daily",
        "Biweekly",
        "Semiannual",
        "Biennial",
        "Triennial",
        "Three times a week",
        "Three times a month",
        "Continuously updated",
        "Monthly",
        "Quarterly",
        "Semimonthly",
        "Three times a year",
        "Weekly",
        "Completely irregular"
      ]
    },
    "bureauCode": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "[0-9]{3}:[0-9]{2}"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "contactPoint": {
      "type": "string"
    },
    "dataDictionary": {
      "type": "string",
      "format": "uri"
    },
    "dataQuality": {
      "type": "boolean"
    },
    "description": {
      "type": "string"
    },
    "distribution": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "accessURL": {
            "type": "string",
            "format": "uri"
          },
          "format": {
            "pattern": "^[-a-fA-F0-9]+/[-a-fA-F0-9]+([+][-a-fA-F0-9]+)?$",
            "type": "string"
          }
        }
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "identifier": {
      "type": "string",
      "pattern": "[0-9a-fA-F]+"
    },
    "issued": {
      "type": "string",
      "format": "date-time"
    },
    "keyword": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "landingPage": {
      "type": "string",
      "format": "uri"
    },
    "language": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[a-zA-Z]+(-[a-zA-Z]+)*$"
      }
    },
    "license": {
      "type": "string"
    },
    "mbox": {
      "type": "string",
      "format": "email"
    },
    "modified": {
      "type": "string",
      "format": "date-time"
    },
    "PrimaryITInvestmentUII": {
      "type": "string"
    },
    "programCode": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "[0-9]{3}:[0-9]{3}"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "publisher": {
      "type": "string"
    },
    "references": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uri"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "spatial": {
      "type": "string"
    },
    "systemOfRecords": {
      "type": "string"
    },
    "temporal": {
      "type": "array"
    },
    "theme": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "title": {
      "type": "string"
    },
    "webService": {
      "type": "string",
      "format": "uri"
    }
  }
}