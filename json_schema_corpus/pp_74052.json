        },
        "info": {
          "$ref": "container.json#/definitions/info"
        },
        "project": {
          "$ref": "#/definitions/project"
        },
        "uid": {
          "$ref": "container.json#/definitions/uid"
        },
        "timestamp": {
          "$ref": "container.json#/definitions/timestamp"
        },
        "timezone": {
