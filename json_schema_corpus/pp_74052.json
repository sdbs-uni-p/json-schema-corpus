          "type": "array",
          "items": {
            "$ref": "job.json#/definitions/job-output"
          }
        },
        "containers": {
          "type": "object",
          "patternProperties": {
            "^[a-fA-F0-9]{24}$": {
              "type": "object"
            }
          }
        }
      },
      "additionalProperties": false
    },
    "session-template-recalc-output": {
      "type": "object",
      "properties": {
        "sessions_changed": {
          "type": "array",
          "items": {
            "allOf": [
              {
                "$ref": "common.json#/definitions/objectid"
              }
            ]
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "sessions_changed"
      ]
    }
  }
