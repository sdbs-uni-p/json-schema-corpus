{
  "id": "/GroupedTimeseries",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "axes": {
      "type": "object",
      "properties": {
        "x": {
          "$ref": "#/definitions/axis"
        },
        "y": {
          "type": "array",
          "minItems": 1,
          "items": {
            "$ref": "#/definitions/axis"
          }
        }
      }
    },
    "axis-period": {
      "type": "string",
      "required": false,
      "oneOf": [
        {
          "enum": [
            "day",
            "week",
            "month",
            "quarter"
          ]
        }
      ]
    },
    "date-picker": {
      "type": "object",
      "required": false,
      "properties": {
        "start-date": {
          "type": "string",
          "format": "date-time",
          "required": true
        }
      }
    },
    "value-attribute": {
      "required": true
    },
    "format": {
      "oneOf": [
        {
          "type": "object",
          "required": false,
          "additionalProperties": false,
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "duration"
              ]
            },
            "unit": {
              "type": "string",
              "enum": [
                "m"
              ]
            }
          }
        }
      ]
    },
    "use_stack": {
      "type": "boolean"
    },
    "show-line-labels": {
      "type": "boolean"
    },
    "show-total-label": {
      "type": "boolean"
    },
    "one-hundred-percent": {
      "type": "boolean"
    },
    "group-mapping": {
      "type": "object"
    }
  },
  "definitions": {
    "axis": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "key": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "array"
            }
          ]
        },
        "format": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string"
                }
              }
            }
          ]
        }
      }
    }
  },
  "$schema": "http://json-schema.org/draft-03/schema#"
}