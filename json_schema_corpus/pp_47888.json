{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "id": "https://inowas.com/schema/",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
    },
    "layer_id": {
      "type": "string",
      "pattern": "^[a-z0-9_\\-]{1,255}$",
      "maxLength": 255
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "layer_id"
  ]
}