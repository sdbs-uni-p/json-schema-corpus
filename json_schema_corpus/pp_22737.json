{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "$license": [
    "http://www.apache.org/licenses/LICENSE-2.0"
  ],
  "id": "#",
  "javaType": "org.apache.streams.pipl.pojo.Url",
  "properties": {
    "@source_id": {
      "type": "string",
      "description": "The source ID (if this URL is a source of data)"
    },
    "@domain": {
      "type": "string",
      "description": "The domain"
    },
    "@name": {
      "type": "string",
      "description": "The site name."
    },
    "@category": {
      "type": "string",
      "description": "One of: 'background_reports\u2019, 'contact_details\u2019, 'email_address\u2019, 'media\u2019, 'personal_profiles\u2019, 'professional_and_business\u2019, 'public_records\u2019, 'publications\u2019, 'school_and_classmates\u2019, 'web_pages\u2019"
    },
    "url": {
      "type": "string",
      "description": "The actual URL"
    },
    "@sponsored": {
      "type": "boolean",
      "description": "Indicating whether additional data from this source is available behind a website paywall. Omitted if false."
    }
  }
}