{
  "$schema": "http://json-schema.org/schema#",
  "title": "Event",
  "type": "object",
  "properties": {
    "level": {
      "type": "string",
      "enum": [
        "debug",
        "info",
        "warn",
        "error",
        "critical"
      ]
    },
    "source": {
      "type": "string"
    },
    "time": {
      "format": "date-time",
      "type": "string"
    },
    "message": {
      "type": "string"
    }
  },
  "additionalProperties": false
}