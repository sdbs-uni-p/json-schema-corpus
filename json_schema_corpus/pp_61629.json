{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "PayloadUUID": {
      "default": "",
      "pattern": "^[0-9A-Za-z]{8}-[0-9A-Za-z]{4}-[0-9A-Za-z]{4}-[0-9A-Za-z]{4}-[0-9A-Za-z]{12}$",
      "type": "string",
      "description": "Unique identifier for the payload (format 01234567-89AB-CDEF-0123-456789ABCDEF)"
    },
    "PayloadDescription": {
      "default": "Adds a PKCS#1-formatted certificate",
      "type": "string",
      "description": "Description of the payload"
    },
    "PayloadContent": {
      "type": "string",
      "description": "Mandatory. The base64 representation of the payload with a line length of 52."
    },
    "PayloadOrganization": {
      "type": "string",
      "description": "This value describes the issuing organization of the profile, as displayed to the user"
    },
    "PayloadIdentifier": {
      "default": "com.apple.security.pkcs1",
      "type": "string",
      "description": "A unique identifier for the payload, dot-delimited.  Usually root PayloadIdentifier+subidentifier"
    },
    "PayloadDisplayName": {
      "default": "Certificate",
      "type": "string",
      "description": "Name of the payload"
    },
    "PayloadType": {
      "default": "com.apple.security.pkcs1",
      "type": "string",
      "description": "The type of the payload, a reverse dns string"
    },
    "PayloadCertificateFileName": {
      "type": "string",
      "description": "The filename for the certificate"
    },
    "PayloadVersion": {
      "default": 1,
      "type": "number",
      "description": "The version of the whole configuration profile."
    },
    "Password": {
      "type": "string",
      "description": "Password protecting the PKCS12 file."
    }
  },
  "title": "com.apple.security.pkcs1"
}