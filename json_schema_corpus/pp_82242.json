{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Transitland Datastore Changeset",
  "type": "object",
  "properties": {
    "changes": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": [
          "action"
        ],
        "oneOf": [
          {
            "required": [
              "feed"
            ],
            "properties": {
              "feed": {
                "$ref": "./feed.json"
              }
            }
          },
          {
            "required": [
              "gtfsRealtimeFeed"
            ],
            "properties": {
              "feed": {
                "$ref": "./feed.json"
              }
            }
          },
          {
            "required": [
              "stop"
            ],
            "properties": {
              "stop": {
                "$ref": "./stop.json"
              }
            }
          },
          {
            "required": [
              "stopPlatform"
            ],
            "properties": {
              "stopPlatform": {
                "$ref": "./stop.json"
              }
            }
          },
          {
            "required": [
              "stopEgress"
            ],
            "properties": {
              "stopPlatform": {
                "$ref": "./stop.json"
              }
            }
          },
          {
            "required": [
              "operator"
            ],
            "properties": {
              "operator": {
                "$ref": "./operator.json"
              }
            }
          },
          {
            "required": [
              "route"
            ],
            "properties": {
              "route": {
                "$ref": "./route.json"
              }
            }
          },
          {
            "required": [
              "scheduleStopPair"
            ],
            "properties": {
              "scheduleStopPair": {
                "$ref": "./schedule_stop_pair.json"
              }
            }
          },
          {
            "required": [
              "routeStopPattern"
            ],
            "properties": {
              "routeStopPattern": {
                "$ref": "./route_stop_pattern.json"
              }
            }
          }
        ],
        "properties": {
          "action": {
            "type": "string",
            "enum": [
              "createUpdate",
              "destroy",
              "changeOnestopID",
              "changeStopType",
              "merge"
            ]
          },
          "issuesResolved": {
            "type": "array",
            "items": {
              "type": "number"
            }
          }
        }
      }
    }
  }
}