{
  "$schema": "http://json-schema.org/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "pattern": "^\\d{1,3}$"
      },
      "lat": {
        "type": "string",
        "pattern": "^\\d{1,3}\\.\\d{2,6}$"
      },
      "lon": {
        "type": "string",
        "pattern": "^\\d{1,3}\\.\\d{2,6}$"
      },
      "address": {
        "type": "string"
      },
      "addressNumber": {
        "type": "string"
      },
      "bikes": {
        "type": "string",
        "pattern": "^\\d{1,2}$"
      },
      "slots": {
        "type": "string",
        "pattern": "^\\d{1,2}$"
      },
      "status": {
        "type": "string",
        "enum": [
          "OPN",
          "CLS"
        ]
      },
      "stationType": {
        "type": "string",
        "enum": [
          "BIKE",
          "ELECTRIC_BIKE"
        ]
      }
    },
    "required": [
      "id",
      "district",
      "lon",
      "lat",
      "bikes",
      "slots",
      "zip",
      "address",
      "nearbyStations",
      "status",
      "name",
      "stationType"
    ]
  }
}