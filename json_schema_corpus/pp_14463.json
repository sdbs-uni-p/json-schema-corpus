{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Action target",
  "type": "object",
  "properties": {
    "id": {
      "description": "Unique identifier given by the server",
      "type": "integer"
    },
    "generatedIdentifier": {
      "description": "Unique id generated by the server.",
      "type": "string"
    },
    "organizationId": {
      "description": "The organization.",
      "type": "integer"
    },
    "name": {
      "description": "A name to identify the action target.",
      "type": "string"
    },
    "public": {
      "description": "Make the action target available to everybody.",
      "type": "boolean"
    },
    "actionTargetTemplateId": {
      "description": "The action target template instantiated.",
      "type": "integer"
    },
    "deletable": {
      "type": "boolean",
      "description": "Define if the action target can be deleted."
    },
    "configuration": {
      "description": "A valid JSON object.",
      "type": "json"
    }
  },
  "required": [
    "id",
    "actionTargetId",
    "name",
    "actionTargetTemplateId",
    "deletable",
    "configuration"
  ]
}