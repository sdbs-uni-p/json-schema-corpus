{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "id": "https://inowas.com/schema/",
  "properties": {
    "uuid": {
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
    },
    "message_name": {
      "type": "string",
      "pattern": "^addBoundary$"
    },
    "metadata": {
      "type": "object"
    },
    "payload": {
      "$ref": "modflow/command/addBoundaryPayload.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "uuid",
    "message_name",
    "metadata",
    "payload"
  ]
}