{
  "$schema": "http://json-schema.org/draft-04/schema",
  "id": "http://schema.dcos/acl/users/user/groups",
  "title": "DCOS ACL: Groups a user is member of",
  "type": "object",
  "properties": {
    "array": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "membershipurl": {
            "title": "Membership API endpoint",
            "type": "string"
          },
          "group": {
            "allOf": [
              {
                "$ref": "http://schema.dcos/acl/groups/group"
              }
            ]
          }
        },
        "required": [
          "membershipurl",
          "group"
        ]
      }
    }
  },
  "required": [
    "array"
  ]
}