{
  "$schema": "http://json-schema.org/schema#",
  "self": {
    "vendor": "com.nordstrom",
    "name": "product-photos/assignment-record",
    "format": "jsonschema",
    "version": "1-0-0"
  },
  "type": "object",
  "properties": {
    "path": {
      "type": "string",
      "pattern": "^/product-photos$"
    },
    "httpMethod": {
      "type": "string",
      "pattern": "^POST$"
    },
    "headers": {
      "type": "object",
      "properties": {
        "X-Twilio-Signature": {
          "type": "string",
          "pattern": "^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)$"
        }
      },
      "additionalProperties": true
    },
    "body": {
      "type": "object",
      "properties": {
        "From": {
          "type": "string",
          "pattern": "^\\+?[1-9]\\d{1,14}$"
        },
        "NumMedia": {
          "type": "string",
          "pattern": "^\\d+$"
        },
        "MediaUrl0": {
          "type": "string",
          "format": "url"
        },
        "Body": {
          "type": "string"
        }
      },
      "additionalProperties": true
    }
  },
  "required": [
    "path",
    "httpMethod",
    "headers"
  ],
  "additionalProperties": true
}