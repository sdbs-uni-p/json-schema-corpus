{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Spectrum Grant parameters provided by a CBSD",
  "type": "object",
  "required": [
    "cbsdId",
    "operationParam"
  ],
  "properties": {
    "cbsdId": {
      "type": "string",
      "description": "Unique designator for the CBSD as returned from the registration procedure."
    },
    "operationParam": {
      "$ref": "file:OperationParam.schema.json"
    },
    "measReport": {
      "$ref": "file:MeasReport.schema.json"
    }
  }
}