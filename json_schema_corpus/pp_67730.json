{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "StartTransactionResponse",
  "type": "object",
  "properties": {
    "transactionId": {
      "type": "integer"
    },
    "idTagInfo": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string",
          "enum": [
            "Accepted",
            "Blocked",
            "Expired",
            "Invalid",
            "ConcurrentTx"
          ]
        },
        "expiryDate": {
          "type": "string",
          "format": "date-time"
        },
        "parentIdTag": {
          "type": "string",
          "maxLength": 20
        }
      },
      "required": [
        "status"
      ]
    }
  },
  "required": [
    "transactionId",
    "idTagInfo"
  ]
}