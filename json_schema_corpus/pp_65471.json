{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "source_url": {
      "description": "Place where this fact can be verified",
      "type": "string"
    },
    "sample_date": {
      "description": "Date on which this fact was known to be true",
      "type": "string",
      "format": "date"
    }
  },
  "required": [
    "source_url",
    "sample_date"
  ],
  "additionalProperties": true
}