{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "version": "1.2.1",
  "type": "object",
  "definitions": {
    "object": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "source": {
          "type": "string"
        },
        "page": {
          "type": "integer"
        },
        "size": {
          "type": "string"
        },
        "creatureType": {
          "$ref": "util.json#/definitions/creatureType"
        },
        "objectType": {
          "type": "string",
          "description": "siege weapon; generic; unknown",
          "enum": [
            "SW",
            "GEN",
            "U"
          ]
        },
        "str": {
          "type": "integer"
        },
        "dex": {
          "type": "integer"
        },
        "con": {
          "type": "integer"
        },
        "int": {
          "type": "integer"
        },
        "wis": {
          "type": "integer"
        },
        "cha": {
          "type": "integer"
        },
        "ac": {
          "type": "string"
        },
        "hp": {
          "type": "string"
        },
        "speed": {
          "$ref": "util.json#/definitions/speed"
        },
        "senses": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          ]
        },
        "resist": {
          "$ref": "util.json#/definitions/damageResistArray"
        },
        "conditionImmune": {
          "$ref": "util.json#/definitions/conditionImmunityArray"
        },
        "immune": {
          "$ref": "util.json#/definitions/damageImmunityArray"
        },
        "vulnerable": {
          "$ref": "util.json#/definitions/damageVulnerabilityArray"
        },
        "entries": {
          "type": "array",
          "items": {
            "$ref": "entry.json"
          }
        },
        "actionEntries": {
          "type": "array",
          "items": {
            "$ref": "entry.json"
          }
        },
        "tokenUrl": {
          "type": "string"
        },
        "srd": {
          "$ref": "util.json#/definitions/srd"
        },
        "isNpc": {
          "type": "boolean"
        },
        "hasToken": {
          "type": "boolean"
        }
      },
      "additionalProperties": false,
      "required": [
        "name",
        "source",
        "size",
        "objectType",
        "hp",
        "immune"
      ]
    }
  },
  "properties": {
    "object": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/object"
      }
    }
  },
  "additionalProperties": false
}