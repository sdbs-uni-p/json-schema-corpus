{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Organizations",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "The identifier of the organization."
      },
      "name": {
        "type": "string",
        "description": "The name of the organization."
      },
      "deletable": {
        "type": "boolean",
        "description": "Define if the organization can be deleted."
      }
    },
    "required": [
      "name"
    ]
  }
}