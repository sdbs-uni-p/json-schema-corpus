{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "version": "1.7.0",
  "title": "Optional Features",
  "type": "object",
  "definitions": {
    "prerequisite": {
      "$ref": "util.json#/definitions/prerequisite"
    },
    "featureType": {
      "type": "string",
      "enum": [
        "ED",
        "EI",
        "MM",
        "MV",
        "MV:B",
        "OTH",
        "FS:F",
        "FS:B",
        "FS:R",
        "FS:P",
        "MV:C2-UA",
        "AS:V1-UA",
        "AS:V2-UA",
        "AS",
        "PB",
        "AI",
        "SHP:H",
        "SHP:M",
        "SHP:W",
        "SHP:F",
        "SHP:O",
        "IWM:W",
        "IWM:A",
        "IWM:G",
        "OR",
        "RN",
        "AF"
      ]
    },
    "optionalfeature": {
      "properties": {
        "name": {
          "type": "string"
        },
        "prerequisite": {
          "$ref": "#/definitions/prerequisite"
        },
        "entries": {
          "type": "array",
          "items": {
            "$ref": "entry.json"
          }
        },
        "previousVersion": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "source": {
              "type": "string"
            }
          },
          "required": [
            "name",
            "source"
          ],
          "additionalProperties": false
        },
        "source": {
          "type": "string"
        },
        "page": {
          "type": "integer"
        },
        "featureType": {
          "oneOf": [
            {
              "$ref": "#/definitions/featureType"
            },
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/featureType"
              }
            }
          ]
        },
        "otherSources": {
          "$ref": "util.json#/definitions/otherSources"
        },
        "srd": {
          "$ref": "util.json#/definitions/srd"
        }
      },
      "required": [
        "name",
        "entries",
        "source",
        "featureType"
      ],
      "additionalProperties": false
    }
  },
  "properties": {
    "optionalfeature": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/optionalfeature"
      }
    }
  },
  "additionalProperties": false
}