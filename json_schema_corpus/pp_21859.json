{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "Name"
  ],
  "properties": {
    "Name": {
      "type": "string",
      "pattern": ".+"
    },
    "BaseApiId": {
      "type": "string",
      "pattern": "^[a-z0-9]{10}$"
    },
    "Description": {
      "type": "string",
      "pattern": ".+"
    }
  },
  "additionalProperties": false
}