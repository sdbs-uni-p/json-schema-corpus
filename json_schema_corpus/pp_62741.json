{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "checkbox",
  "description": "A checkbox atom with optional label",
  "type": "object",
  "properties": {
    "tag": {
      "enum": [
        "div",
        "li",
        "span"
      ]
    },
    "label": {
      "type": "string"
    },
    "inputId": {
      "type": "string"
    },
    "inputName": {
      "type": "string"
    },
    "inputValue": {
      "type": "string"
    },
    "checked": {
      "type": "boolean"
    },
    "state": {
      "enum": [
        "",
        "disabled"
      ]
    }
  },
  "required": [
    "tag",
    "inputId",
    "inputName",
    "inputValue"
  ],
  "dependencies": {}
}