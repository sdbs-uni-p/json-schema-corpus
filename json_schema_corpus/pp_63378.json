{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "format": "minimal",
  "properties": {
    "full_name": {
      "type": "string",
      "pattern": "^[^/#]+/[^/#]+#[0123456789]+$"
    },
    "number": {
      "format": "positiveInteger"
    },
    "name": {
      "type": "string",
      "pattern": "^[^/#]+#[0123456789]+$"
    },
    "repository_full_name": {
      "type": "string",
      "pattern": "^[^/#]+/[^/#]+$"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "full_name",
    "number",
    "name",
    "repository_full_name",
    "created_at"
  ]
}