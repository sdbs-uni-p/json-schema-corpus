            "type": "number",
            "maximum": 1
          },
          "init_seg_smooth": {
            "default": 0.0001,
            "minimum": 0.0001,
            "type": "number",
            "maximum": 0.1
          },
          "no_pve": {
            "type": "boolean"
          },
          "bias_lowpass": {
