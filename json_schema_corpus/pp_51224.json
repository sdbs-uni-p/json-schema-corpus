{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "timestamp",
  "description": "The formats used for all timestamps and timestamp precisions in TWIGS",
  "type": "string",
  "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}\\.[0-9]{6}[\\+-][0-9]{2}:[0-9]{2}$",
  "definitions": {
    "precision": {
      "type": "string",
      "enum": [
        "year",
        "month",
        "day",
        "hour",
        "minute",
        "second"
      ]
    }
  }
}