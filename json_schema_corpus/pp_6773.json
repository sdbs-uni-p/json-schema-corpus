{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema#",
  "required": true,
  "properties": {
    "session_id": {
      "type": "string",
      "required": true
    },
    "trace_seq": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "required": true,
      "items": {
        "type": "object",
        "required": true,
        "properties": {
          "location": {
            "type": "object",
            "required": true,
            "properties": {
              "accuracy": {
                "type": "number",
                "required": true
              },
              "bounds": {
                "type": "object",
                "required": true,
                "properties": {
                  "northEast": {
                    "type": "object",
                    "required": true,
                    "properties": {
                      "lat": {
                        "type": "number",
                        "minimum": -90,
                        "maximum": 90,
                        "required": true
                      },
                      "lng": {
                        "type": "number",
                        "minimum": -180,
                        "maximum": 180,
                        "required": true
                      }
                    }
                  },
                  "southWest": {
                    "type": "object",
                    "required": true,
                    "properties": {
                      "lat": {
                        "type": "number",
                        "minimum": -90,
                        "maximum": 90,
                        "required": true
                      },
                      "lng": {
                        "type": "number",
                        "minimum": -180,
                        "maximum": 180,
                        "required": true
                      }
                    }
                  }
                }
              },
              "latlng": {
                "type": "object",
                "required": true,
                "properties": {
                  "lat": {
                    "type": "number",
                    "minimum": -90,
                    "maximum": 90,
                    "required": true
                  },
                  "lng": {
                    "type": "number",
                    "minimum": -180,
                    "maximum": 180,
                    "required": true
                  }
                }
              }
            }
          },
          "routes": {
            "type": "object",
            "required": true,
            "properties": {
              "num": {
                "type": "number",
                "required": true
              }
            }
          },
          "timestamp": {
            "type": "string",
            "format": "date-time",
            "required": true
          }
        }
      }
    }
  }
}