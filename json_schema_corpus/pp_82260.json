              },
              "amount": {
                "type": "number"
              }
            },
            "required": [
              "description",
              "base",
              "rate",
              "amount"
            ]
          }
        },
        "commissionable_price": {
          "type": "number"
        },
        "commission_amount": {
          "type": "number"
        },
        "override_commission_amount": {
          "type": "number"
        },
        "total": {
          "type": "number"
        }
      },
      "required": [
        "NonCommissionableService",
        "TourismTaxes",
        "FiscalTaxes",
        "commissionable_price",
        "commission_amount",
        "override_commission_amount",
        "total"
      ]
    }
