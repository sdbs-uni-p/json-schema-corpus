              }
            },
            "required": [
              "description",
              "amount"
            ]
          }
        },
        "TourismTaxes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "description": {
                "type": "string"
              },
              "base": {
                "type": "number"
              },
              "rate": {
                "type": "number"
              },
              "amount": {
                "type": "number"
              }
            },
            "required": [
              "description",
              "base",
              "rate",
              "amount"
            ]
          }
        },
        "FiscalTaxes": {
          "type": "array",
