    "Operator": {
      "type": [
        "object",
        "null"
      ],
      "title": "Operador del paquete reservado.",
      "properties": {
        "id": {
          "type": "integer",
          "title": "Identificador del operador."
        },
        "name": {
          "type": "string",
          "title": "Nombre del operador."
        },
        "cuit": {
          "type": "string",
          "title": "CUIT del operador."
        }
      },
      "required": [
        "id",
        "name",
        "cuit"
      ]
    },
