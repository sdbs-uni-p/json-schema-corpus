{
  "fullname": "Detector State",
  "experiment": "CMS",
  "jsonschema": {
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Detector State",
    "type": "object",
    "description": "Information concerning the state of the detector",
    "properties": {
      "measurement_description": {
        "type": "string",
        "title": "Measurement Description"
      },
      "code_base": {
        "$ref": "https://analysispreservation.cern.ch/schemas/definitions/cms-user-code-v0.0.1.json"
      },
      "detailed_measurement_description": {
        "type": "string",
        "title": "Description Details"
      },
      "n_tuple": {
        "$ref": "https://analysispreservation.cern.ch/schemas/definitions/cms-input-code-output-v0.0.1.json"
      },
      "detector_final_state": {
        "type": "object",
        "properties": {
          "veto": {
            "items": {
              "type": "object",
              "properties": {
                "pt_cut": {
                  "type": "string",
                  "title": "PT"
                },
                "number": {
                  "pattern": "^[\\d\\.]+$",
                  "type": "string",
                  "title": "Number"
                },
                "particle": {
                  "enum": [
                    "electron",
                    "muon",
                    "tau",
                    "jet",
                    "bjet",
                    "photon",
                    "track",
                    "vertex",
                    "MET",
                    "HT"
                  ],
                  "type": "string",
                  "title": "Physics Object"
                },
                "eta_cut": {
                  "type": "string",
                  "title": "ETA"
                }
              }
            },
            "type": "array",
            "description": "Provide vetos",
            "title": "Vetos"
          },
          "final_state_particles": {
            "default": [
              {}
            ],
            "items": {
              "type": "object",
              "properties": {
                "jet_type": {
                  "enum": [
                    "AK4PF",
                    "AK5PF",
                    "AK6PF",
                    "AK7PF",
                    "AK8PF",
                    "AK4Calo",
                    "AK5Calo",
                    "AK6Calo",
                    "AK7Calo",
                    "AK8Calo",
                    "KT4PF",
                    "KT5PF",
                    "KT6PF",
                    "KT7PF",
                    "KT8PF",
                    "KT4Calo",
                    "KT5Calo",
                    "KT6Calo",
                    "KT7Calo",
                    "KT8Calo"
                  ],
                  "type": "string",
                  "title": "Jet type"
                },
                "photon_type": {
                  "enum": [
                    "Photon",
                    "Conversion"
                  ],
                  "type": "string",
                  "title": "Photon type"
                },
                "number_vertex": {
                  "type": "object",
                  "properties": {
                    "number": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": "Number"
                    },
                    "sign": {
                      "enum": [
                        "="
                      ],
                      "type": "string",
                      "title": "="
                    }
                  },
                  "title": "Number"
                },
                "track_type": {
                  "enum": [
                    "Heavy-Ion",
                    "pp"
                  ],
                  "type": "string",
                  "title": "Track type"
                },
                "number": {
                  "type": "object",
                  "properties": {
                    "number": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": "Number"
                    },
                    "sign": {
                      "default": "=",
                      "enum": [
                        "<",
                        ">",
                        "=",
                        "<=",
                        ">="
                      ],
                      "type": "string",
                      "title": "<, >, =, <=, >="
                    }
                  },
                  "title": "Number"
                },
                "tau_discriminator": {
                  "type": "object",
                  "properties": {
                    "tag": {
                      "enum": [
                        "AgainstElectronLooseMVA",
                        "other"
                      ],
                      "type": "string",
                      "title": "Tag"
                    },
                    "custom_tag": {
                      "type": "string",
                      "title": " "
                    },
                    "value": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": "Value"
                    }
                  },
                  "title": "Discriminator"
                },
                "number_tracks": {
                  "type": "object",
                  "properties": {
                    "number": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": ">"
                    }
                  },
                  "title": "Number of Tracks"
                },
                "tau_type": {
                  "enum": [
                    "PFTau"
                  ],
                  "type": "string",
                  "title": "Tau type"
                },
                "muon_type": {
                  "enum": [
                    "PFMuon",
                    "GlobalMuon",
                    "TrackerMuon"
                  ],
                  "type": "string",
                  "title": "Muon type"
                },
                "pt_cut": {
                  "default": [],
                  "items": {
                    "type": "object",
                    "properties": {
                      "number": {
                        "pattern": "^[\\d\\.]+$",
                        "type": "string",
                        "title": "GeV"
                      },
                      "sign": {
                        "default": ">",
                        "enum": [
                          ">",
                          "=",
                          "<"
                        ],
                        "type": "string",
                        "title": "<, >, ="
                      }
                    }
                  },
                  "type": "array",
                  "title": "pT Cuts"
                },
                "eta_cut": {
                  "default": [],
                  "items": {
                    "type": "object",
                    "properties": {
                      "number": {
                        "enum": [
                          "1.1",
                          "1.5",
                          "2.4",
                          "2.5",
                          "other"
                        ],
                        "type": "string",
                        "title": " "
                      },
                      "other_number": {
                        "pattern": "^[\\d\\.]+$",
                        "type": "string",
                        "title": " "
                      },
                      "sign": {
                        "default": ">",
                        "enum": [
                          ">",
                          "=",
                          "<"
                        ],
                        "type": "string",
                        "title": "<, >, ="
                      }
                    }
                  },
                  "type": "array",
                  "title": "|\u03b7| Cuts"
                },
                "jet_discriminator": {
                  "type": "object",
                  "properties": {
                    "tag": {
                      "enum": [
                        "other"
                      ],
                      "type": "string",
                      "title": "Tag"
                    },
                    "custom_tag": {
                      "type": "string",
                      "title": " "
                    },
                    "value": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": "Value"
                    }
                  },
                  "title": "Discriminator"
                },
                "electron_discriminator": {
                  "type": "object",
                  "properties": {
                    "tag": {
                      "enum": [
                        "other"
                      ],
                      "type": "string",
                      "title": "Tag"
                    },
                    "custom_tag": {
                      "type": "string",
                      "title": " "
                    },
                    "value": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": "Value"
                    }
                  },
                  "title": "Discriminator"
                },
                "met_pf_corrections": {
                  "enum": [
                    "Type0PFCorrections",
                    "Type1PFCorrections",
                    "Type2PFCorrections"
                  ],
                  "type": "string",
                  "title": "MET Corrections"
                },
                "jet_corrections": {
                  "enum": [
                    "JetCorrections"
                  ],
                  "type": "string",
                  "title": "Jet Corrections"
                },
                "met_type": {
                  "enum": [
                    "PFMET",
                    "CaloMET"
                  ],
                  "type": "string",
                  "title": "MET type"
                },
                "object": {
                  "enum": [
                    "",
                    "electron",
                    "muon",
                    "tau",
                    "jet",
                    "bjet",
                    "photon",
                    "track",
                    "vertex",
                    "MET",
                    "HT"
                  ],
                  "type": "string",
                  "title": "Object"
                },
                "met_calo_corrections": {
                  "enum": [
                    "Type0CaloCorrections",
                    "Type1CaloCorrections",
                    "Type2CaloCorrections"
                  ],
                  "type": "string",
                  "title": "MET Corrections"
                },
                "muon_discriminator": {
                  "type": "object",
                  "properties": {
                    "tag": {
                      "enum": [
                        "other"
                      ],
                      "type": "string",
                      "title": "Tag"
                    },
                    "custom_tag": {
                      "type": "string",
                      "title": " "
                    },
                    "value": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": "Value"
                    }
                  },
                  "title": "Discriminator"
                },
                "sel_criteria": {
                  "enum": [
                    "tight",
                    "medium",
                    "loose",
                    "other"
                  ],
                  "type": "string",
                  "title": "Selection Criteria"
                },
                "isolation": {
                  "type": "object",
                  "properties": {
                    "calorimeter": {
                      "type": "object",
                      "properties": {
                        "deltaRs": {
                          "pattern": "^[\\d\\.]+$",
                          "type": "string",
                          "title": "\u0394R <"
                        },
                        "pTs": {
                          "pattern": "^[\\d\\.]+$",
                          "type": "string",
                          "title": "pT <"
                        }
                      },
                      "title": "calorimeter"
                    },
                    "notracks": {
                      "type": "object",
                      "properties": {
                        "pTg": {
                          "pattern": "^[\\d\\.]+$",
                          "type": "string",
                          "title": "pT >"
                        },
                        "deltaRs": {
                          "pattern": "^[\\d\\.]+$",
                          "type": "string",
                          "title": "\u0394R <"
                        }
                      },
                      "title": "no tracks"
                    }
                  },
                  "title": "Isolation"
                },
                "photon_discriminator": {
                  "type": "object",
                  "properties": {
                    "tag": {
                      "enum": [
                        "other"
                      ],
                      "type": "string",
                      "title": "Tag"
                    },
                    "custom_tag": {
                      "type": "string",
                      "title": " "
                    },
                    "value": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": "Value"
                    }
                  },
                  "title": "Discriminator"
                },
                "bjet_discriminator": {
                  "type": "object",
                  "properties": {
                    "tag": {
                      "enum": [
                        "trackCountingHighPurBJetTags",
                        "trackCountingHighEffBJetTags",
                        "simpleSecondaryVertexHighEffBJetTags",
                        "simpleSecondaryVertexHighEffBJetTags",
                        "combinedSecondaryVertexBJetTags",
                        "jetProbabilityBJetTags",
                        "other"
                      ],
                      "type": "string",
                      "title": "Tag"
                    },
                    "custom_tag": {
                      "type": "string",
                      "title": " "
                    },
                    "value": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": "Value"
                    }
                  },
                  "title": "Discriminator"
                },
                "electron_type": {
                  "enum": [
                    "GsfElectron"
                  ],
                  "type": "string",
                  "title": "Electron type"
                }
              }
            },
            "type": "array",
            "title": "Physics Objects"
          },
          "final_state_relations": {
            "items": {
              "type": "object",
              "properties": {
                "physics_objects": {
                  "enum": [],
                  "type": "string",
                  "title": "Physics Objects Available"
                },
                "relation_type": {
                  "enum": [
                    "charge",
                    "angle",
                    "invariant_mass",
                    "transverse_mass"
                  ],
                  "type": "string",
                  "title": "Relation Type"
                },
                "invariant_mass": {
                  "type": "object",
                  "properties": {
                    "upper_range": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": ">"
                    },
                    "lower_range": {
                      "type": "number",
                      "title": "<"
                    }
                  },
                  "title": "GeV"
                },
                "charge_relation": {
                  "enum": [
                    "same_sign",
                    "opposite_sign"
                  ],
                  "type": "string",
                  "title": "Charge"
                },
                "angle_relation": {
                  "pattern": "^[\\d\\.]+$",
                  "type": "string",
                  "title": "Degrees"
                },
                "transverse_mass": {
                  "type": "object",
                  "properties": {
                    "upper_range": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": ">"
                    },
                    "lower_range": {
                      "pattern": "^[\\d\\.]+$",
                      "type": "string",
                      "title": "<"
                    }
                  },
                  "title": "GeV"
                }
              }
            },
            "type": "array",
            "title": "Relations"
          }
        },
        "title": "Event Selection"
      }
    }
  },
  "is_deposit": false
}