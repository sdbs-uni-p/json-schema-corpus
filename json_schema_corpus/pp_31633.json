        "SSL",
        "SSL_BRIDGE",
        "SSL_TCP",
        "DTLS",
        "NNTP",
        "RPCSVR",
        "DNS",
        "ADNS",
        "SNMP",
        "RTSP",
        "DHCPRA",
        "ANY",
        "SIP_UDP",
        "DNS_TCP",
        "ADNS_TCP",
        "MYSQL",
        "MSSQL",
        "RADIUS",
        "RDP",
        "DIAMETER",
        "SSL_DIAMETER",
        "TFTP"
