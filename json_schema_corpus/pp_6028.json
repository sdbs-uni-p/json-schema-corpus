{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "username": {
      "type": "string",
      "format": "username"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "firstName": {
      "type": "string",
      "format": "firstName"
    },
    "lastName": {
      "type": "string"
    },
    "createdAt": {
      "type": "string",
      "format": "datePast"
    },
    "updatedAt": {
      "type": "string",
      "format": "datePast"
    },
    "profile": {
      "type": "object",
      "properties": {
        "biography": {
          "type": "string"
        }
      },
      "required": [
        "biography"
      ]
    },
    "auth_provider": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "authId": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "authId"
        ]
      }
    }
  },
  "required": [
    "id",
    "username",
    "email",
    "firstName",
    "lastName",
    "createdAt",
    "updatedAt",
    "profile"
  ]
}