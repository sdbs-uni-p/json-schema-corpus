{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "id": "#",
  "javaType": "com.sysomos.SysomosConfiguration",
  "properties": {
    "apiKey": {
      "type": "string"
    },
    "endpoint": {
      "type": "string",
      "description": "The endpoint",
      "default": "http://api.sysomos.com/"
    },
    "heartbeatIds": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    },
    "minDelayMs": {
      "type": "String",
      "format": "utc-millisec"
    },
    "scheduledDelayMs": {
      "type": "String",
      "format": "utc-millisec"
    },
    "maxBatchSize": {
      "type": "String",
      "format": "utc-millisec"
    },
    "apiBatchSize": {
      "type": "String",
      "format": "utc-millisec"
    }
  }
}