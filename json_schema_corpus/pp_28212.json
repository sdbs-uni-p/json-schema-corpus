{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "primaryKey": "{{ city }}{{ state }}",
  "properties": {
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "zip": {
      "type": "integer"
    },
    "county": {
      "type": "string"
    },
    "country": {
      "type": "string"
    }
  },
  "required": [
    "city",
    "state",
    "zip",
    "county",
    "country"
  ]
}