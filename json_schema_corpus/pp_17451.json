{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Self-employment source of income",
  "type": "object",
  "properties": {
    "id": {
      "title": "ID",
      "description": "Unique identifier",
      "type": "string"
    },
    "accountName": {
      "title": "Account name",
      "description": "Friendly account name",
      "type": "string",
      "pattern": "^([a-zA-Z0-9]+\\s?)+$",
      "minLength": 1,
      "maxLength": 32
    }
  },
  "required": [
    "id"
  ],
  "additionalProperties": false
}