{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://toyatech.net/cmsapp/events/Event",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "type": {
      "$ref": "http://toyatech.net/cmsapp/events/EventType"
    },
    "startDateTime": {
      "type": "string",
      "format": "date-time"
    },
    "endDateTime": {
      "type": "string",
      "format": "date-time"
    },
    "attendance": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/AttendanceRecord"
      }
    }
  },
  "required": [
    "title",
    "type"
  ]
}