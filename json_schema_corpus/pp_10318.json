{
  "$schema": "http://json-schema.org/draft-04/schema",
  "id": "link-collection.json",
  "readOnly": true,
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "href": {
        "type": "string",
        "format": "uri"
      },
      "description": {
        "type": "string"
      },
      "rel": {
        "type": "string"
      }
    },
    "required": [
      "href",
      "rel"
    ]
  }
}