{
  "$schema": "http://json-schema.org/schema#",
  "title": "CacheSettings",
  "type": "object",
  "properties": {
    "settings": {
      "properties": {
        "nodes": {
          "type": "array",
          "minItems": 1,
          "items": {
            "port": {
              "type": "integer"
            },
            "host": {
              "type": "string"
            },
            "required": [
              "host",
              "port"
            ]
          }
        }
      }
    }
  },
  "required": [
    "settings"
  ]
}