{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "wellsets": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "label": {
            "type": "string"
          },
          "size": {
            "type": "integer"
          },
          "wells": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "index": {
                  "type": "string"
                },
                "values": {
                  "type": "array",
                  "items": {
                    "type": "number"
                  }
                }
              },
              "required": [
                "index",
                "values"
              ]
            }
          }
        },
        "required": [
          "type",
          "label",
          "size",
          "wells"
        ]
      }
    }
  },
  "required": [
    "wellsets"
  ]
}