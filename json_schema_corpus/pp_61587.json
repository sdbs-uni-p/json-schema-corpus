{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "PayloadUUID": {
      "default": "",
      "pattern": "^[0-9A-Za-z]{8}-[0-9A-Za-z]{4}-[0-9A-Za-z]{4}-[0-9A-Za-z]{4}-[0-9A-Za-z]{12}$",
      "type": "string",
      "description": "Unique identifier for the payload (format 01234567-89AB-CDEF-0123-456789ABCDEF)"
    },
    "PayloadDescription": {
      "default": "Sidebar Preferences",
      "type": "string",
      "description": "Description of the payload"
    },
    "hideiCloudDrive": {
      "type": "boolean",
      "description": "Hide iCloud Drive"
    },
    "PayloadOrganization": {
      "type": "string",
      "description": "This value describes the issuing organization of the profile, as displayed to the user"
    },
    "PayloadIdentifier": {
      "default": "com.apple.MCX.sidebar",
      "type": "string",
      "description": "A unique identifier for the payload, dot-delimited.  Usually root PayloadIdentifier+subidentifier"
    },
    "PayloadDisplayName": {
      "default": "Sidebar Preferences",
      "type": "string",
      "description": "Name of the payload"
    },
    "PayloadType": {
      "default": "com.apple.MCX.sidebar",
      "type": "string",
      "description": "The type of the payload, a reverse dns string"
    },
    "shouldAddUserHome": {
      "type": "boolean",
      "description": "Add User Home"
    },
    "PayloadVersion": {
      "default": 1,
      "type": "number",
      "description": "The version of the whole configuration profile."
    },
    "customBaseURLs": {
      "items": {},
      "type": "array",
      "description": "Base URLs to custom locations."
    },
    "shouldAddNetworkHome": {
      "type": "boolean",
      "description": "Add Network Home"
    },
    "SidebarDestination": {
      "type": "string",
      "description": "Unknown, Maybe after before of custom urls"
    }
  },
  "title": "com.apple.MCX.sidebar"
}